{
    "request": {
        "method": "PUT",
        "urlPathPattern": "/cft/api/v1/transfers/(.*)/ack",
        "queryParameters": {
            "idm": {
                "matches": ".*"
            }
        },
        "bodyPatterns": [
            {
                "matchesJsonPath": "$.msg"
            }
        ],
        "headers": {
            "Accept": {
                "equalTo": "application/json"
            }
        },
        "basicAuth": {
            "username": "admin",
            "password": "Passw0rd"
        }
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        }
    }
}