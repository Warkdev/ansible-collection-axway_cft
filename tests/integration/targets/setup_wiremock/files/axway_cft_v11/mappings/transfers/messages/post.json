{
    "priority": 1,
    "request": {
        "method": "POST",
        "urlPath": "/transfers/messages",
        "queryParameters": {
            "part": {
                "matches": ".*"
            },
            "idm": {
                "matches": ".*"
            },
            "apitimeout": {
                "or": [
                    {
                        "matches": "[0-9]+"
                    },
                    {
                        "absent": true
                    }
                ]
            }
        },
        "bodyPatterns": [
            {
                "matchesJsonPath": "$.msg"
            }
        ],
        "headers": {
            "Accept": {
                "equalTo": "application/json"
            }
        },
        "basicAuth": {
            "username": "admin",
            "password": "Passw0rd"
        }
    },
    "response": {
        "status": 201,
        "jsonBody": {
            "partner": "PARIS",
            "idf": "T12345",
            "idtu": "IDTU1234",
            "state": "A",
            "phase": "K",
            "phasestep": "Z",
            "ida": "T1234567",
            "diagi": 99,
            "diagp": "0",
            "links": {
                "rel": "self",
                "href": "https://mycft/transfers/IDTU1234"
            }
        },
        "headers": {
            "Content-Type": "application/json"
        }
    }
}