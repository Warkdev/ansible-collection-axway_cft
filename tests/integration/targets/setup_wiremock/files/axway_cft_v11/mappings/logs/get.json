{
    "request": {
        "method": "GET",
        "urlPathPattern": "/cft/api/v1/logs",
        "queryParameters": {
            "severity": {
                "or": [
                    {
                        "matches": "^[F|E|W|I]$"
                    },
                    {
                        "absent": true
                    }
                ]
            },
            "limit": {
                "or": [
                    {
                        "matches": "^[0-9]+$"
                    },
                    {
                        "absent": true
                    }
                ]
            },
            "datetimemin": {
                "or": [
                    {
                        "matches": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
                    },
                    {
                        "absent": true
                    }
                ]
            },
            "datetimemax": {
                "or": [
                    {
                        "matches": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
                    },
                    {
                        "absent": true
                    }
                ]
            },
            "pattern": {
                "or": [
                    {
                        "matches": "^.*$"
                    },
                    {
                        "absent": true
                    }
                ]
            }
        },
        "headers": {
            "Accept": {
                "equalTo": "application/json"
            }
        },
        "basicAuth": {
            "username": "admin",
            "password": "Passw0rd"
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "logs": [
                {
                    "node": "mynode",
                    "severity": "F",
                    "code": "messagecode",
                    "date": "2014-08-29T12:45:08Z",
                    "message": "sample log"
                },
                {
                    "node": "mynode",
                    "severity": "F",
                    "code": "messagecode",
                    "date": "2014-08-30T12:45:08Z",
                    "message": "another log"
                }
            ]
        },
        "headers": {
            "Content-Type": "application/json"
        }
    }
}